<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ua.kas.main.MainController">
   <children>
      <Label fx:id="isConnected" alignment="CENTER" layoutX="121.0" layoutY="22.0" prefHeight="28.0" prefWidth="300.0" text="status" textAlignment="CENTER" textFill="RED" AnchorPane.leftAnchor="0.0">
         <font>
            <Font name="Times New Roman Bold Italic" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="login" layoutX="28.0" layoutY="88.0" promptText="login">
         <font>
            <Font name="Times New Roman" size="12.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" layoutX="28.0" layoutY="127.0" promptText="password">
         <font>
            <Font name="Times New Roman" size="12.0" />
         </font>
      </PasswordField>
      <Button layoutX="193.0" layoutY="88.0" mnemonicParsing="false" onAction="#login" prefHeight="64.0" prefWidth="85.0" text="Button">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
      <Button layoutX="193.0" layoutY="161.0" mnemonicParsing="false" onAction="#test" prefHeight="25.0" prefWidth="85.0" text="test">
         <font>
            <Font name="Times New Roman" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
